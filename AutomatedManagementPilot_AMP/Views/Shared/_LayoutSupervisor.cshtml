                                    </div>
                                </div>
                                <script src="~/lib/signalr/dist/browser/signalr.js"></script>
                                <script src="~/js/chat.js"></script>

                            </div>
                        </e-content-template>
                    </ejs-sidebar>
                    <!-- end of sidebar element -->
                    <!-- main content declaration -->
 
                </div>
                <title>Essential JS 2 (Preview) for ASP.Net Core Sidebar-> Dock</title>
            </div>
